<block wx:if="{{msgList.length==0}}"><view class="content" style="{{'height:'+(freeHeight+'px')+';'}}"><image src="../../static/image/message/nomessage.jpg" mode></image></view></block><block wx:else><view class="container"><block wx:for="{{msgList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><tui-swipe-action vue-id="{{'1-'+index}}" actions="{{actions}}" params="{{item}}" data-event-opts="{{[['^click',[['handlerButton']]]]}}" bind:click="__e" bind:__l="__l" vue-slots="{{['content']}}"><view slot="content"><tui-list-cell vue-id="{{('2-'+index)+','+('1-'+index)}}" last="{{true}}" index="{{item.id}}" data-event-opts="{{[['^click',[['detail']]]]}}" bind:click="__e" bind:__l="__l" vue-slots="{{['default']}}"><view class="tui-msg-box"><image class="tui-msg-pic" src="{{'../../static/image/news/'+item.pic+'.jpg'}}" mode="widthFix"></image><view class="tui-msg-item"><view class="tui-msg-name">{{item.nickname}}</view><view class="tui-msg-content">{{item.msg}}</view></view></view><view class="{{['tui-msg-right',item.level==3?'tui-right-dot':'']}}"><view class="tui-msg-time">{{item.time}}</view><block wx:if="{{item.msgNum>0}}"><tui-badge vue-id="{{('3-'+index)+','+('2-'+index)}}" type="danger" bind:__l="__l" vue-slots="{{['default']}}">{{item.msgNum}}</tui-badge></block></view></tui-list-cell></view></tui-swipe-action></block></view></block>